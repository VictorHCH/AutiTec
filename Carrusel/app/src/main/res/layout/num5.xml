<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Num1"
    android:background="@color/_bg__inicio_ek3_color">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/backnum5"
            android:layout_width="60dp"
            android:layout_height="60dp"

            android:layout_alignParentStart="true"

            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:src="@drawable/flecha_back" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/likeImageView"
            android:layout_width="158dp"
            android:layout_height="72dp"
            android:layout_margin="16dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/burbuja" />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/like2ImageView"
            android:layout_width="177dp"
            android:layout_height="148dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="-75dp"
            app:srcCompat="@drawable/burbuja"

            />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/like3ImageView"
            android:layout_width="180dp"
            android:layout_height="119dp"
            android:layout_margin="16dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/burbuja" />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/like4ImageView"
            android:layout_width="175dp"
            android:layout_height="143dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="-75dp"
            app:srcCompat="@drawable/burbuja" />

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/like5ImageView"
            android:layout_width="180dp"
            android:layout_height="106dp"
            android:layout_margin="16dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/burbuja" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>